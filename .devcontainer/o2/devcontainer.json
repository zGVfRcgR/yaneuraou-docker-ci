{
  "name": "YaneuraOu -O2",
  "image": "ghcr.io/zgvfrcgr/yaneuraou:o2",
  "remoteUser": "root",
  "workspaceFolder": "/workspace",
  "mounts": [
    "source=${localWorkspaceFolder},target=/workspace,type=bind"
  ],
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.defaultProfile.linux": "bash"
      },
      "extensions": []
    }
  },
  "forwardPorts": [4081],
  "postCreateCommand": [
    "./YaneuraOu-by-gcc", "usi",
    "echo 'YaneuraOu -O2 devcontainer ready!'"
  ]
}